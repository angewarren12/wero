{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "js/**",
      "use": "@vercel/static"
    },
    {
      "src": "css/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/users",
      "dest": "/api/users.js"
    },
    {
      "src": "/api/users/(.*)",
      "dest": "/api/users.js?code=$1"
    },
    {
      "handle": "filesystem"
    }
  ]
}
